<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="accountdao.AccountManpper">
    <select id="findUser" parameterType="int" resultType="int">
        select count(*) from fb_word;
    </select>
    <insert id="insertWord">
        insert into fb_word (`word`, `id`) values (#{word}, #{id})
    </insert>
</mapper>